<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="DirectoryMonitorPath" value="D:\Tmp\FilePool" />
    <add key="DirectoryMonitorFileMask" value="In???File.txt" />
    <!--
    <add key="RRDMAgentOrigin" value="T24" />
    <add key="RRDMAgentSourceFileID" value="T24FT01" />
      -->
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
  <connectionStrings>
    <add name="AgentConnectionString" connectionString="Server=localhost;Database=RRDM_AGENT;User ID=ATMUser" providerName="System.Data.SqlClient" />
    <add name="ATMSConnectionString" connectionString="Server=localhost;Database=ATMS;User ID=ATMUser" providerName="System.Data.SqlClient" />
    <add name="ReconConnectionString" connectionString="Server=localhost;Database=RRDM_Reconciliation;User ID=ATMUser" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <system.web>
    <membership defaultProvider="ClientAuthenticationMembershipProvider">
      <providers>
        <add name="ClientAuthenticationMembershipProvider" type="System.Web.ClientServices.Providers.ClientFormsAuthenticationMembershipProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" />
      </providers>
    </membership>
    <roleManager defaultProvider="ClientRoleProvider" enabled="true">
      <providers>
        <add name="ClientRoleProvider" type="System.Web.ClientServices.Providers.ClientRoleProvider, System.Web.Extensions, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" serviceUri="" cacheTimeout="86400" />
      </providers>
    </roleManager>
  </system.web>
</configuration>